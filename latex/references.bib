@article{johanssonCanMineralOil2020,
  title = {Can {{Mineral Oil Slicks Be Distinguished From Newly Formed Sea Ice Using Synthetic Aperture Radar}}?},
  author = {Johansson, A. Malin and Espeseth, Martine M. and Brekke, Camilla and Holt, Benjamin},
  year = {2020},
  journal = {IEEE Journal of Selected Topics in Applied Earth Observations and Remote Sensing},
  volume = {13},
