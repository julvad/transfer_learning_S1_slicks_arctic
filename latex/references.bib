@article{lundschienNorthBarentsComposite2025,
  title = {North {{Barents Composite Tectono-Sedimentary Element}}},
  author = {Lundschien, B. A. and Mattingsdal, R. and Johansen, S. K. and Knutsen, S.-M.},
  year = {2025},
  month = oct,
  journal = {Geological Society, London, Memoirs},
  volume = {57},
